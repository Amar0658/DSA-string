#include <stdio.h>
/* void or int or char*/strleng(char *str)
{
int length=0;
while(*str!='\0')
{
length++;
str++;
}
return printf("The length of string is: %d",length);
}
int main()
{
char str[50];
printf("Enter the string: ");
gets(str);
strleng;
}

orb 

#include <stdio.h>
int main ()
{
char str[]={'a','v','z','y','t'};
int length=sizeof(str)/sizeof(str[0]);
printf("Size of string is: %d",length);
return 0;
}
